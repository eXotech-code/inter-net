	"bugs": {
		"url": "https://github.com/eXotech-code/inter-net/issues"
	},
	"homepage": "https://github.com/eXotech-code/inter-net#readme",
	"dependencies": {
		"express": "^4.17.1",
		"mocha": "^7.1.1",
		"nodemon": "^2.0.4",
		"socket.io": "^2.3.0"
	},
	"devDependencies": {
		"node-sass": "^4.14.1",
		"prettier": "2.1.2"
	}
